<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/activity_second_ime_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ime" />

    <TextView
        android:id="@+id/activity_second_prezime_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/prezime" />

    <TextView
        android:id="@+id/activity_second_adresa_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/adresa" />

</LinearLayout>
